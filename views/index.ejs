<!DOCTYPE html>
<html>
    <head>
        <title><%= title %></title>
    </head>
    <body>
        <h1><%= title %></h1>
        <p>EJS Welcome to <%= title %></p>
        <script src="/plugin/ajax.js"></script>
        <script>
            getUserInfo({ id: 1 });

            async function getUserInfo(form) {
                const { name, age, sex } = await ajax.post("/users/", form);

                console.log(name, age, sex);
            }
        </script>
    </body>
</html>
